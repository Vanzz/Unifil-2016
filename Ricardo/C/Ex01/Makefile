Ex01_3: Ex01_3.c
	$(CC) Ex01_3.c -o Ex01_3

Ex01_4: Ex01_4.c
	$(CC) Ex01_4.c -o Ex01_4

Ex01_5: Ex01_5.c
	$(CC) Ex01_5.c -o Ex01_5

clean:
	rm -rf Ex01_1 Ex01_2 Ex01_3 Ex01_4 Ex01_5
